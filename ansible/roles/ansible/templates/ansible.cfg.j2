# Ansible Configuration File
# Generated by Ansible role

[defaults]
{% if ansible_config.defaults is defined %}
{% for key, value in ansible_config.defaults.items() %}
{{ key }} = {{ value }}
{% endfor %}
{% endif %}

{% if ansible_config.privilege_escalation is defined %}
[privilege_escalation]
{% for key, value in ansible_config.privilege_escalation.items() %}
{{ key }} = {{ value }}
{% endfor %}
{% endif %}

{% if ansible_config.ssh_connection is defined %}
[ssh_connection]
{% for key, value in ansible_config.ssh_connection.items() %}
{{ key }} = {{ value }}
{% endfor %}
{% endif %}